; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Yozora Maker"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

Score="*res://Score.gd"

[display]

window/size/width=136
window/size/height=240
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="PlayerLife"
2d_physics/layer_3="PlayerGraze"
2d_physics/layer_4="Star"
2d_physics/layer_5="Stage"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.0196078, 0, 0.0941176, 1 )
environment/default_environment="res://default_env.tres"
